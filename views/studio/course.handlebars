{{> nav_studio}}



<div class="about-title d-flex justify-content-center align-items-center">
  <p>課程一覽</p>
</div>

<form action="/{{ studio.subdomain }}/course" method="GET" id="select-week">
  <input type="week" id="week" name="week" value="{{theYear}}-W{{theWeek}}">
  <button type="submit">查詢</button>
</form>



<div class="d-flex flex-column" id="curriculum">
  
  {{!-- 日期 --}}
  <section id="title-section" class="d-flex align-items-center justify-content-between align-items-stretch">

    <div class="period d-flex flex-column align-items-center"></div>

    <div class="period d-flex flex-column align-items-center">
      <span>{{organizedCourseDetailList.Monday.date}}</span>
      <span>週一</span>
    </div>

    <div class="period d-flex flex-column align-items-center">
      <span>{{organizedCourseDetailList.Tuesday.date}}</span>
      <span>週二</span>
    </div>

    <div class="period d-flex flex-column align-items-center">
      <span>{{organizedCourseDetailList.Wednesday.date}}</span>
      <span>週三</span>
    </div>

    <div class="period d-flex flex-column align-items-center">
