    </div>

    <div class="period d-flex flex-column align-items-center">
      <span>{{organizedCourseDetailList.Sunday.date}}</span>
      <span>週日</span>
    </div>
  </section>




  {{!-- Morning --}}
  <section class="d-flex align-items-center justify-content-between align-items-stretch time-slot-section">

    <div class="period time-slot">
      <span>Morning</span>
    </div>

    <div class="period">
      {{#each organizedCourseDetailList.Monday.morning}}
      <div class="d-flex flex-column align-items-center course-detail design-1">
        <span class="course-title">{{this.title}} - {{this.point}} 點</span>
        <span class="course-teacher">（{{this.name}}）</span>
        <span class="course-time">{{this.start_time}}-{{this.end_time}}</span>


        <div class="d-flex align-items-center book-div">
          <span class="course-limitation">空位: {{this.limitation}}</span>

          {{#canBook this.limitation}}
          <form action="/{{../../studio.subdomain}}/registration" method="GET" class="d-flex align-items-center">
            <input type="number" id="courseDetailId" name="courseDetailId" value="{{../this.id}}" style="display: none">
            <input type="number" id="isOnline" name="isOnline" value=0 style="display: none">
            <input type="number" id="isOneOnOne" name="isOneOnOne" value=0 style="display: none">
            <button type="submit" class="register-course">預約</button>
          </form>
          {{else}}
          <button class="empty">預約已滿</button>
          {{/canBook}}
        </div>
        

        <div class="d-flex align-items-center book-div">
          {{#compare this.is_online 1}}
          <span class="course-limitation">線上空位: {{../this.online_limitation}}</span>
            {{#canBook ../this.online_limitation}}
            <form action="/{{../../studio.subdomain}}/registration" method="GET" class="d-flex align-items-center">
              <input type="number" id="courseDetailId" name="courseDetailId" value="{{../this.id}}" style="display: none">
              <input type="number" id="isOnline" name="isOnline" value=1 style="display: none">
              {{#compare ../this.is_oneOnOne 0}}
              <input type="number" id="isOneOnOne" name="isOneOnOne" value=0 style="display: none">
              {{else}}
              <input type="number" id="isOneOnOne" name="isOneOnOne" value=0 style="display: none">
              {{/compare}}
